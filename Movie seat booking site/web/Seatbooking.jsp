            <div class="seat available" data-seat="M83" data-price="1500" onclick="selectSeat(this)">M83</div>
            <div class="seat available" data-seat="M84" data-price="1500" onclick="selectSeat(this)">M84</div>
            <div class="seat available" data-seat="M85" data-price="1500" onclick="selectSeat(this)">M85</div>
            <div class="seat available" data-seat="M86" data-price="1500" onclick="selectSeat(this)">M86</div>
        </div>
       
    </div>
</div>

  <div class="legend">
    <div class="item">
      <span class="seat available"></span> Available
    </div>
    <div class="item">
      <span class="seat selected"></span> Selected
    </div>
    <div class="item">
      <span class="seat reserved"></span> Reserved
    </div>
  </div>

    <!-- Info Button to Trigger Notification -->
    <button class="info-button" onclick="showNotification()">Seat Layout Info</button>

    <!-- Notification Pop-up -->
    <div class="overlay" id="overlay" onclick="hideNotification()"></div>
    <div class="notification" id="notification">
        <button class="close-btn" onclick="hideNotification()">X</button>
        <p>The seat layout is for illustrative purposes only. The actual spacing between seats, distance from the screen, and screen size may differ from what is depicted in the illustration.</p>
    </div>
  <div class="footer">
    <div class="seat-info" id="seat-info">Seat Info: None</div>
     <div class="total-price">Total[LKR] : <span id="total-price">0</span></div>
  </div>

  <button id="reserve-button" class="reserve-button" onclick="reserveSeats()">Reserve Seats</button>

  <script src="script.js"></script>
</body>
</html>
